<!-- Page Preloder -->
<div id="preloder">
  <div class="loader"></div>
</div>

<!-- Humberger Begin -->


<!-- Categories Section Begin -->
<section class="categories">
  <div class="container">
    <div class="row">
      <div class="categories__slider owl-carousel">
        {{#each category}}
        <div class="col-lg-3">
          <div class="categories__item set-bg" data-setbg="/category-images/{{this._id}}.jpg">
            <h5><a href="#">{{this.category}}</a></h5>
          </div>
        </div>
        {{/each}}
      </div>
    </div>
  </div>
</section>
<!-- Categories Section End -->

<!-- Featured Section Begin -->
<section class="featured spad">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <div class="section-title">
          <h2>Featured Products</h2>
        </div>
        <div class="featured__controls">
          <ul>
            <li class="active" data-filter="*">All</li>
            <li data-filter=".oranges">Oranges</li>
            <li data-filter=".fresh-meat">Fresh Meat</li>
            <li data-filter=".vegetables">Vegetables</li>
            <li data-filter=".fastfood">Fastfood</li>
          </ul>
        </div>
      </div>
    </div>
    <div class="row featured__filter">
      {{#each product}}
      {{#if featured}}

      <div class="col-lg-3 col-md-4 col-sm-6 mix oranges fresh-meat">
        <div class="product__discount__item">
          <div class="product__discount__item__pic set-bg" data-setbg="/product-images/{{this._id}}.jpg">
            <div class="product__discount__percent">{{this.percentage}}%<a>Off</a>
            </div>
            <ul class="product__item__pic__hover">
              <li><a href="/add-to-wishlist?id={{this._id}}"><i class="fa fa-heart"></i></a></li>
              <li><a href="#"><i class="fa fa-retweet"></i></a></li>
              <li><a onclick="return addToCart('{{this._id}}')"><i class="fa fa-shopping-cart"></i></a></li>
            </ul>
          </div>
          <div class="product__discount__item__text">
            <span>{{this.category}}</span>
            <h5><a href="/product-details?id={{this._id}}">{{this.productName}}</a></h5>
            <div class="product__item__price">₹{{this.OurPrice}}.00
              <span>₹{{this.marketPrice}}.00</span>
            </div>
          </div>
        </div>
      </div>

      {{/if}}
      {{/each}}



    </div>
  </div>
</section>
<!-- Featured Section End -->

<!-- Banner Begin -->
<div class="banner">
  <div class="container">
    <div class="row">
      <div class="col-lg-6 col-md-6 col-sm-6">
        <div class="banner__pic">
          <img src="img/banner/banner-1.jpg" alt="" />
        </div>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-6">
        <div class="banner__pic">
          <img src="img/banner/banner-2.jpg" alt="" />
        </div>
      </div>
    </div>
  </div>
</div>
<!-- Banner End -->

